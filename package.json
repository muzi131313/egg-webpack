{
  "name": "egg-webpack-x",
  "version": "4.2.0",
  "description": "webpack dev server plugin for egg, support read file in memory and hot reload.",
  "eggPlugin": {
    "name": "webpack",
    "env": [
      "local"
    ]
  },
  "keywords": [
    "egg",
    "eggPlugin",
    "egg-plugin",
    "webpack",
    "memory",
    "hot-reload"
  ],
  "dependencies": {
    "mkdirp": "^0.5.1",
    "webpack-tool-x": "3.1.6",
    "co-request": "^0.2.0",
    "iconv-lite": "^0.2.11"
  },
  "devDependencies": {
    "autod": "^2.7.1",
    "egg": "^1.0.0",
    "egg-bin": "^4.1.0",
    "egg-ci": "^1.5.0",
    "egg-mock": "^3.2.0",
    "eslint": "^4.5.0",
    "eslint-config-egg": "^5.0.0",
    "supertest": "^3.0.0",
    "webstorm-disable-index": "^1.1.2",
    "conventional-changelog-cli": "^1.3.5"
  },
  "engines": {
    "node": ">=6.0.0"
  },
  "scripts": {
    "test": "npm run lint -- --fix && npm run test-local",
    "test-local": "egg-bin test",
    "cov": "egg-bin cov",
    "lint": "eslint .",
    "ci": "npm run lint && npm run cov",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s",
    "autod": "autod"
  },
  "files": [
    "index.js",
    "app.js",
    "agent.js",
    "config",
    "app",
    "lib"
  ],
  "ci": {
    "version": "6, 8, 9"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/muzi131313/egg-webpack.git"
  },
  "bugs": {
    "url": "https://github.com/muzi131313/egg-webpack/issues"
  },
  "homepage": "https://github.com/muzi131313/egg-webpack#readme",
  "author": "muzi131313@163.com",
  "license": "MIT"
}
